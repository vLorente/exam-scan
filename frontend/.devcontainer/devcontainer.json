{
  "name": "Exam Scan Frontend",
  "dockerFile": "../Dockerfile",
  "context": "..",
  "workspaceFolder": "/app",
  "features": {
    "ghcr.io/devcontainers/features/node:1": {
      "version": "22"
    }
  },
  "customizations": {
    "vscode": {
      "settings": {
        "terminal.integrated.defaultProfile.linux": "bash"
      },
      "extensions": [
        "angular.ng-template",
        "dbaeumer.vscode-eslint"
      ]
    }
  },
  "forwardPorts": [4200],
  "postCreateCommand": "npm install"
}
